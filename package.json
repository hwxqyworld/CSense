{
  "name": "csense",
  "version": "0.1.4",
  "description": "一个 CCW 安全审计工具",
  "main": "dist/main.js",
  "types": "./dist/index.d.ts",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src",
    "lint:type": "tsc --noEmit -p ./tsconfig.json",
    "fix": "eslint ./src --fix",
    "build": "tsup src/index.js --config tsup.config.release.ts && node ./script/build.mjs",
    "dev": "tsup src/index.js --config tsup.config.dev.ts",
    "format": "prettier ./src *.md -c -w",
    "lint:format": "prettier ./src *.md -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csense-rev/csense-rev.git"
  },
  "author": "FurryR",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/csense-rev/csense-rev/issues"
  },
  "pre-commit": [],
  "homepage": "https://github.com/csense-rev/csense-rev#readme",
  "devDependencies": {
    "@types/node-forge": "^1.3.14",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "jszip": "^3.10.1",
    "prettier": "^3.2.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "node-forge": "^1.3.1",
    "secure-vm": "^1.0.6"
  }
}
